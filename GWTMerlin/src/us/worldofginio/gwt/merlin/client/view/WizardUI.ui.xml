<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder' xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<g:Grid ui:field="wizardGrid">
		<g:row>
			<g:customCell>
				<g:FlowPanel ui:field="pnlContent" styleName="wog-wizardContent"/>
			</g:customCell>
		</g:row>
		<g:row>
			<g:customCell>
				<g:FlowPanel ui:field="pnlButtons" styleName="wog-wizardButtonPanel"/>
			</g:customCell>
		</g:row>
	</g:Grid>

</ui:UiBinder> 